<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/wifi_button"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="50dp"
        android:text="WiFi"
        android:textSize="8sp" />

    <Button
        android:id="@+id/bluetooth_button"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="150dp"
        android:text="BT"
        android:textSize="8sp" />

    <Button
        android:id="@+id/sd_test_button"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="250dp"
        android:text="SD"
        android:textSize="8sp" />

    <Button
        android:id="@+id/sata_test_button"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="350dp"
        android:text="SATA"
        android:textSize="8sp" />

    <Button
        android:id="@+id/usb_test_button"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="450dp"
        android:text="USB"
        android:textSize="8sp" />

    <Button
        android:id="@+id/m2_test_button"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="550dp"
        android:text="M.2"
        android:textSize="8sp" />

    <Button
        android:id="@+id/wifi_status_test_button"
        android:layout_width="80dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="650dp"
        android:text="WIFI ping"
        android:textSize="8sp" />

    <!--
    <Button
        android:id="@+id/check_count_button"
        android:layout_width="80dp"
