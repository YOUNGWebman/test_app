+   private void checkStorageState(String path, String deviceName, TextView statusTextView, TextView spaceTextView) {
+        File directory = new File(path);
+        if (directory.exists()) {
+            // 存储设备已挂载
+            statusTextView.setText(deviceName + " 已挂载");
+            // 获取已使用的容量
+            long usedSpace = getUsedSpace(directory);
+            // 获取总容量
+            long totalSpace = getTotalSpace(directory);
+            spaceTextView.setText("已使用空间：" + formatFileSize(usedSpace) + " / 总容量：" + formatFileSize(totalSpace));
+        } else {
+            // 存储设备未挂载或已被拔出
+            statusTextView.setText(deviceName + " 未挂载或已被拔出");
+            spaceTextView.setText("");
+        }
+    }
