+           return reader.readLine();
+       } catch (IOException e) {
+           e.printStackTrace();
+           return null;
+       } finally {
+           if (reader != null) {
+               try {
+                   reader.close();
+               } catch (IOException e) {
+                   e.printStackTrace();
+               }
+           }
+           if (process != null) {
+               process.destroy();
+           }
+       }
+   }
